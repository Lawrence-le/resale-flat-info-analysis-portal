{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    { "src": "/", "dest": "/" },
    { "src": "/town", "dest": "/town" },
    { "src": "/islandwide", "dest": "/islandwide" }
  ]
}
